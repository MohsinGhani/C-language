#include <stdio.h>

int main()
{
   int row, col, i, j, first[10][10];

   printf("Enter the number of rows of matrix: ");
   scanf("%d", &row);

   printf("Enter the number of columns of matrix: ");
   scanf("%d", &col);

   printf("\n");

   for (i = 0; i < row; i++)
   {
        for (j = 0; j < col; j++)
        {
            printf("Enter the number at index [%d][%d] of first matrix: ",i,j);
            scanf("%d", &first[i][j]);
        }
   }


   printf("\n");


   for (i = 0; i < row; i++) {
      for (j = 0 ; j < col; j++) {
         printf("%d\t", first[i][j]);
      }
      printf("\n");
   }

   return 0;
}
