#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a[50],n,i,j,temp;
    printf("How many numbers: ");
    scanf("%d",&n);
    printf("Enter the numbers\n");
    for(i=0;i<n;i++)
    {
        scanf("%d", &a[i]);
    }
    temp = a[0];
    temp = a[0];
    for(i=1;i<=n-1;i++)
    {
            a[i-1] = a[i];

    }
    a[n-1] = temp;
    printf("the modified array is\n");
    for(i=0;i<=n-1;i++)
    {
        printf("%d\n",a[i]);
    }
    return 0;
}
