#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,row,col;

    printf("Enter rows: ");
    scanf("%d", &row);

    printf("Enter column: ");
    scanf("%d", &col);

    for(i=0; i<row;i++)
    {
        for(j=0; j<col; j++)
        {
        printf("*");
        }
        printf("\n");
    }

    return 0;
}
