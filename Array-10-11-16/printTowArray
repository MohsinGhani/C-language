#include <stdio.h>
#include <stdlib.h>

int main()
{
    int arr[5],i,arr1[5];
    for(i=0;i<=4;i++)
    {
        printf("Enter the value at %d index: ",i);
        scanf("%d", &arr[i]);
    }
    for(i=0;i<=4;i++)
    {
        printf("You Entered value at index %d is: %d\n", i,arr[i]);
    }

    for(i=0;i<=4;i++)
    {
        printf("Enter the value at %d index: ",i);
        scanf("%d", &arr1[i]);
    }
    for(i=0;i<=4;i++)
    {
        printf("You Entered value at index %d is: %d\n", i,arr1[i]);
    }
    return 0;
}
