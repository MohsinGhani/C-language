#include <stdio.h>
#include <stdlib.h>

int main()
{
    int arr[5],i,n,temp = 0;
    for(i=0;i<=4;i++)
    {
        printf("Enter the value at %d index: ",i);
        scanf("%d", &arr[i]);
    }
    printf("Enter a number for search: ");
    scanf("%d", &n);
    for(i=0;i<=4;i++)
    {
        if(arr[i] == n)
        {
            printf("This number is exist at index: %d",i);
            temp = 1;
        }
    }
    if(temp == 0)
    {
        printf("Not Found!");
    }

}
