#include <stdio.h>
#include <conio.h>
//Write a C program to print the table of characters that are equivalent to the ASSCI codes from 1 to 122

int main() {
    int i;

    for(i = 0; i < 26; i++){
       printf("%c = %d   |   %c = %d \n",'A'+i,'A'+i,'a'+i,'a'+i);
    }

    getch();
    return 0;
}
