#include <stdio.h>
#include <stdlib.h>
// 1.Write a C program to print sum of all even numbers between 1 to 50.

int main()
{
    int i, n = 50, sum = 0;
    for(i=1; i<=n;i++)
    {
        if(i%2 != 0)
        {
            printf("%d\n", i);
            sum = sum + i;
        }
    }
    printf("The sum of all even number between 1 to 50 is: %d", sum);
    return 0;
}
