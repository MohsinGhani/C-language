#include <stdio.h>
#include <stdlib.h>
// 5.Write a program to find the factorial of a number using function.

int factorial(int n);

int main()
{
    int num;
    printf("Enter a Number: ");
    scanf("%d", &num);

    int callFunc = factorial(num);
    printf("The factorial of %d is: %d",num, callFunc);
    return 0;
}

int factorial(int num)
{
    int i,f=1;
    for(i=1; i<=num; i++)
    {
        f = f*i;
    }
    return f;
}
