#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c,d,e;
    float avg;
    printf("Enter 5 integers: ");
    scanf("%d", &a);
    scanf("\n%d", &b);
    scanf("\n%d", &c);
    scanf("\n%d", &d);
    scanf("\n%d", &e);
    avg = (a+b+c+d+e) / 5;
    printf("The Avgerage if: %f", avg);
    return 0;
}
